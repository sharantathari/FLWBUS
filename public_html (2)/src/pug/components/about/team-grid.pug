.about-style
  .row.justify-content-center
    include ../cards/profile-card
    - var n = 0
    while n < 9
      .col-md-4.col-sm-6.pa-lg-5.px-2(data-key=n++)
        .wow.fadeInUp(data-wow-delay=(0.2 + (0.1 * n)) + 's' data-wow-duration='0.5s')
          +profile-card(imgAPI.photo[40+n], imgAPI.avatar[n], 'John Dalton', 'Web Designer', 100, 10, 12, 'round', 'portrait')